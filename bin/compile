#!/bin/sh
mkdir /app/yt
echo "curl -L https://yt-dl.org/downloads/latest/youtube-dl -o /app/yt/youtube-dl"
curl -L https://yt-dl.org/downloads/latest/youtube-dl -o /app/yt/youtube-dl
echo "chmod a+rx /app/yt/youtube-dl"
chmod a+rx /app/yt/youtube-dl
pip install --upgrade pip
pip install cowpy pillow pyGithub selenium wikipedia urbandict pySmartDL hexsticker humanize pytube
pip install oauth2client pydrive ffmpeg-python ffmpeg pymongo redis dnspython neofetch-win
pip install google_images_download google-api-python-client googletrans emoji emojis
pip install youtube-dl
