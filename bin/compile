#!/bin/sh
mkdir /app/yt
echo "curl -L https://yt-dl.org/downloads/latest/youtube-dl -o /app/yt/youtube-dl"
curl -L https://yt-dl.org/downloads/latest/youtube-dl -o /app/yt/youtube-dl
echo "chmod a+rx /app/yt/youtube-dl"
chmod a+rx /app/yt/youtube-dl
pip install --upgrade pip
pip install aiohttp-jinja2 aria2p asyncurban babel CairoSVG coffeehouse cowpy cryptg dialog-py Dialogs dnspython emoji 
